
public class TestaCaracteres {
	
	public static void main(String[] args) {
		
		char letra = 'a';
		System.out.println(letra);
		
		char valor = 200;
		System.out.println(valor);
		
		char valor2 = 189;
		System.out.println(valor2);
		
		valor = (char) (valor + 10);
		System.out.println(valor);
		
		String frase = "Rodrigo Sundfeld";
		System.out.println(frase);
	}
	
	

}
